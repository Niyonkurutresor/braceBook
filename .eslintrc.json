{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "no-useless-catch": "off",
        "no-underscore-dangle": "warn",
        "no-plusplus": "warn",
        "linebreak-style": "off",
        "no-console":"warn",
        "no-unused-vars":"warn",
        "no-var":"warn",
        "semi": "warn",
        "quotes":"warn",
        "max-len":"warn"
    }
}
