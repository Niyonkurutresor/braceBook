{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:import/errors",
        "plugin:import/warnings"
      ],
    "plugins": ["import"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
          },
        "babelOptions": {
        "configFile": "./babel.config.js"
        }
    },
    "rules": {
        "comma-dangle": "warn",
        "no-useless-catch": "warn",
        "no-underscore-dangle": "off",
        "linebreak-style": "warn",
        "no-plusplus": "off",
        "no-console":"warn",
        "no-unused-vars":"warn",
        "no-var":"warn",
        "semi": "warn",
        "quotes":"warn",
        "max-len":"warn"
    }
}